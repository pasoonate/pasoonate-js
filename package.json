{
  "name": "pasoonate",
  "version": "1.4.0",
  "description": "DateTime helpers and convert to calendars.",
  "main": "dist/Pasoonate.js",
  "directories": {
    "example": "example",
    "dist": "dist",
    "src": "src"
  },
  "keywords": [
    "calendar",
    "convert",
    "date",
    "time"
  ],
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
